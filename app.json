{
  "expo": {
    "name": "sensationz",
    "slug": "sensationz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "sensationz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/app_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sensationz.sensationzIOS"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sensationz.sensationzANDROID"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-web-browser",
        {
          "experimentalLauncherActivity": true
        }
      ],
      "expo-build-properties",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.535569396954-6j8dqr7ko76igo1e7gnl1bu9uv318p9k"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Rubik-Bold.ttf",
            "./src/assets/fonts/Rubik-ExtraBold.ttf",
            "./src/assets/fonts/Rubik-Medium.ttf",
            "./src/assets/fonts/Rubik-Regular.ttf",
            "./src/assets/fonts/Rubik-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "61e9570e-db82-4e57-b5b1-2393570351d7"
      }
    }
  }
}
